#include<bits/stdc++.h>
using namespace std;
void fun(int arr[],int n)
{
    cout<<"your dynamic array is:";
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
}
int main()
{
   
    int n;
    cout<<"enter the value of n:";
    cin>>n;
    int*arr=new int[n];
    for(int i=0;i<n;i++)
    {
        int data;
        cout<<"enter the value of data:";
        cin>>data;
        arr[i]=data;
    }
    fun(arr,n);
    return 0;
}
